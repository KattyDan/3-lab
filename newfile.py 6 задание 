class Temperature:
    def __init__(self):
        self._celsius = 0
    
    @property
    def celsius(self):
        return self._celsius
    
    @celsius.setter
    def celsius(self, value):
        if -273 <= value <= 1000:
            self._celsius = value
    
    @property
    def fahrenheit(self):
        return self._celsius * 9/5 + 32

# Пример использования
t = Temperature()
t.celsius = 29
print(f"29°C = {t.fahrenheit}°F")